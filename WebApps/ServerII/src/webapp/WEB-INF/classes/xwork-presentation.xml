<xwork>
    <include file="webwork-default.xml"/>
    
    <package name="presentation" extends="webwork-default">
    
    	<global-results>
			<result name="login">/jsp/common/noResponse.jsp</result>
			<result name="input">/jsp/common/noResponse.jsp</result>
			<result name="error">/jsp/common/noResponse.jsp</result>
			<result name="success">/jsp/common/noResponse.jsp</result>
    	</global-results>
    	
		<action name="DMSPresentaion"
					class="com.dimdim.conference.action.presentation.DMSPresentaionAction">
		</action>
    	
		<action name="ControlPresentation"
					class="com.dimdim.conference.action.presentation.PresentationControlAction">
		</action>
    	
    </package>
</xwork>

